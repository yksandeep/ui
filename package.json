{
  "name": "@kumar08/uicomponents",
  "version": "1.0.3",
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/cjs/index.js",
  "homepage": "https://github.com/yksandeep/ui",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8.12.0",
    "yarn": ">=1.22.19",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "test:lib": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn run build:cjs && tsc-alias -p tsconfig.json --debug",
    "build:precommit": "rm -rf testBuild && tsc --outDir testBuild/esm && tsc --module CommonJs --outDir testBuild/cjs && rm -rf testBuild",
    "build:test": "rm -rf testBuild && tsc --outDir testBuild/esm && tsc --module CommonJs --outDir testBuild/cjs",
    "build:esm": "rm -rf ./dist/esm &&  tsc --outDir dist/esm",
    "build:cjs": "rm -rf ./dist/cjs &&  tsc --outDir dist/cjs --module CommonJs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "yalc:publish": "yarn run build && yalc publish --push --no-scripts --yarn",
    "yalc:watch": "nodemon -e js,ts,tsx --ignore 'dist/*/**' --exec \"yarn run build && yalc push --changed\"",
    "build:doc": "rm -rf docs && ./node_modules/.bin/jsdoc --verbose -c ./jsdoc.json",
    "build:doc:watch": "node ./.jsdocbuild/doc_watcher/index.js",
    "create-component": "cd .create-component && yarn && yarn start",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Kumar Sandeep",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@parcel/transformer-typescript-tsc": "^2.8.0",
    "@parcel/validator-typescript": "^2.8.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "^5.1.26",
    "better-docs": "^2.7.2",
    "hound": "^1.0.5",
    "husky": "^8.0.2",
    "jsdoc": "3.6.10",
    "jsdoc-babel": "^0.5.0",
    "nodemon": "^2.0.22",
    "onchange": "^7.1.0",
    "parcel": "^2.8.0",
    "parcel-resolver-ignore": "^2.1.3",
    "parcel-resolver-tspaths": "^0.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsc-alias": "^1.8.4",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.3",
    "typescript-plugin-css-modules": "^4.1.0"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "react-icons": "^4.8.0",
    "styled-components": "^5.3.6",
    "zod": "^3.21.4"
  }
}
